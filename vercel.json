{
    "version": 2,
    "builds": [
        {
            "src": "web/package.json",
            "use": "@vercel/next",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/web/$1"
        }
    ]
}